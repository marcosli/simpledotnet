using <#= ServiceContractsNamespace #>;

namespace <#=this.ServiceImplNamespace #>
{
    public partial class <#= GetTableClassName() #>Service : EntityService<#="<" + GetTableClassName() + ">"#>, I<#= GetTableClassName() #>Service
    {
    }
}

<#+ } 
}#>